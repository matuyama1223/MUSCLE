<div class="imamge-new">
  <%=image_tag "hoge.png", alt: "Rails icon", id: "pic1", class: "image",width:"100%" %>
<div class="imamge-back"> 
<div class="container">
<div class="row">
<div class="col-md-4 col-md-offset-4">

<h2 class="text-danger">MUSCLE新規登録</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="form-group has-icon">
    <%= f.text_field :name, autofocus: true, class:"form-control", placeholder:"名前"%>
  </div>

  <div class="form-group has-icon">
    <%= f.email_field :email, autofocus: true,class:"form-control", placeholder:"メールアドレス" %>
  </div>

  <div class="form-group has-icon">
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password",class:"form-control", placeholder:"パスワード"  %>
  </div>

  <div class="form-group has-icon">
    <%= f.password_field :password_confirmation, autocomplete: "new-password",class:"form-control", placeholder:"パスワード確認"   %>
  </div>

  <div class="form-group has-icon">
    <%= f.submit "サインアップ" ,class:"btn btn-success" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>
</div>
</div>
</div>
